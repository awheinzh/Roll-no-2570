#include <iostream>
using namespace std;

int main() {
    // Loop from 1 to 10
    for (int i = 1; i <= 10; i++) {
        if (i % 2 == 0) {
            continue;  // Skip the even numbers
        }
        cout << i << " ";  // Only odd numbers will be printed
    }

    cout << "\nLoop ended." << endl;
    return 0;
}
