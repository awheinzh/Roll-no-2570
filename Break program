#include <iostream>
using namespace std;

int main() {
    int i;

    // Loop from 1 to 10
    for (i = 1; i <= 10; i++) {
        if (i == 5) {
            cout << "Loop stopped at i = " << i << endl;
            break;  // Exit the loop when i is 5
        }
        cout << "i = " << i << endl;
    }

    cout << "Loop ended." << endl;
    return 0;
}
